{
  "id": "tiny-gpt2-test",
  "displayName": "Tiny GPT-2 Test",
  "hfModelId": "sshleifer/tiny-gpt2",
  "servedModelName": "sshleifer/tiny-gpt2-test",
  "storageUri": "pvc://venus-model-storage/test-cli",
  "runtime": "vllm-runtime",
  "nodeSelector": {
    "kubernetes.io/hostname": "venus"
  },
  "tolerations": [
    {
      "key": "amd.com/gpu",
      "operator": "Exists",
      "effect": "NoSchedule"
    }
  ],
  "resources": {
    "requests": {
      "cpu": "2",
      "memory": "4Gi",
      "amd.com/gpu": "1"
    },
    "limits": {
      "cpu": "4",
      "memory": "8Gi",
      "amd.com/gpu": "1"
    }
  },
  "env": [
    {
      "name": "HF_HOME",
      "value": "/mnt/models/.hf-cache"
    },
    {
      "name": "MODEL_ID",
      "value": "sshleifer/tiny-gpt2"
    },
    {
      "name": "MODEL_DIR",
      "value": "/mnt/models"
    },
    {
      "name": "HUGGING_FACE_HUB_TOKEN",
      "valueFrom": {
        "secretKeyRef": {
          "name": "hf-token",
          "key": "HUGGING_FACE_HUB_TOKEN"
        }
      }
    }
  ],
  "vllm": {
    "tensorParallelSize": 1,
    "dtype": "float16",
    "gpuMemoryUtilization": 0.5,
    "maxModelLen": 2048
  }
}
