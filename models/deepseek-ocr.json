{
  "id": "deepseek-ocr",
  "displayName": "DeepSeek OCR",
  "hfModelId": "deepseek-ai/DeepSeek-OCR",
  "servedModelName": "deepseek-ai/DeepSeek-OCR",
  "storageUri": "pvc://venus-model-storage/deepseek-ai/DeepSeek-OCR",
  "runtime": "vllm-runtime",
  "nodeSelector": {
    "kubernetes.io/hostname": "venus"
  },
  "tolerations": [
    {
      "key": "amd.com/gpu",
      "operator": "Exists",
      "effect": "NoSchedule"
    }
  ],
  "resources": {
    "requests": {
      "cpu": "6",
      "memory": "16Gi",
      "amd.com/gpu": "1"
    },
    "limits": {
      "cpu": "10",
      "memory": "24Gi",
      "amd.com/gpu": "1"
    }
  },
  "env": [
    {
      "name": "HF_HOME",
      "value": "/mnt/models/.hf-cache"
    },
    {
      "name": "MODEL_ID",
      "value": "deepseek-ai/DeepSeek-OCR"
    },
    {
      "name": "MODEL_DIR",
      "value": "/mnt/models"
    },
    {
      "name": "HUGGING_FACE_HUB_TOKEN",
      "valueFrom": {
        "secretKeyRef": {
          "name": "hf-token",
          "key": "HUGGING_FACE_HUB_TOKEN"
        }
      }
    }
  ],
  "vllm": {
    "tensorParallelSize": 1,
    "dtype": "float16",
    "gpuMemoryUtilization": 0.9,
    "maxModelLen": 4096,
    "trustRemoteCode": true,
    "extraArgs": [
      "--swap-space",
      "8"
    ]
  }
}
