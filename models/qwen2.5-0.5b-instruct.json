{
  "id": "qwen2.5-0.5b-instruct",
  "displayName": "Qwen2.5 0.5B Instruct",
  "hfModelId": "Qwen/Qwen2.5-0.5B-Instruct",
  "runtime": "qwen-vllm-runtime",
  "nodeSelector": {
    "kubernetes.io/hostname": "venus"
  },
  "tolerations": [
    {
      "key": "amd.com/gpu",
      "operator": "Exists",
      "effect": "NoSchedule"
    }
  ],
  "resources": {
    "requests": {
      "cpu": "4",
      "memory": "8Gi",
      "amd.com/gpu": "1"
    },
    "limits": {
      "cpu": "8",
      "memory": "16Gi",
      "amd.com/gpu": "1"
    }
  },
  "env": [
    {
      "name": "HF_HOME",
      "value": "/mnt/models/.hf-cache"
    },
    {
      "name": "TRANSFORMERS_CACHE",
      "value": "/mnt/models/.hf-cache"
    }
  ],
  "volumeMounts": [
    {
      "name": "model-storage",
      "mountPath": "/mnt/models"
    }
  ],
  "volumes": [
    {
      "name": "model-storage",
      "persistentVolumeClaim": {
        "claimName": "venus-model-storage"
      }
    }
  ],
  "vllm": {
    "dtype": "float16",
    "gpuMemoryUtilization": 0.9,
    "maxModelLen": 4096,
    "tensorParallelSize": 1
  }
}
