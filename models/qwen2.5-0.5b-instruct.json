{
  "id": "qwen2.5-0.5b-instruct",
  "displayName": "Qwen2.5 0.5B Instruct",
  "hfModelId": "Qwen/Qwen2.5-0.5B-Instruct",
  "storageUri": "pvc://venus-model-storage",
  "runtime": "qwen-vllm-runtime",
  "nodeSelector": {
    "kubernetes.io/hostname": "venus"
  },
  "tolerations": [
    {
      "key": "amd.com/gpu",
      "operator": "Exists",
      "effect": "NoSchedule"
    }
  ],
  "resources": {
    "requests": {
      "cpu": "4",
      "memory": "8Gi",
      "amd.com/gpu": "1"
    },
    "limits": {
      "cpu": "8",
      "memory": "16Gi",
      "amd.com/gpu": "1"
    }
  },
  "env": [
    {
      "name": "HF_HOME",
      "value": "/mnt/models/.hf-cache"
    },
    {
      "name": "MODEL_ID",
      "value": "Qwen/Qwen2.5-0.5B-Instruct"
    },
    {
      "name": "HUGGING_FACE_HUB_TOKEN",
      "valueFrom": {
        "secretKeyRef": {
          "name": "hf-token",
          "key": "HUGGING_FACE_HUB_TOKEN"
        }
      }
    }
  ],
  "vllm": {
    "dtype": "float16",
    "gpuMemoryUtilization": 0.9,
    "maxModelLen": 4096,
    "tensorParallelSize": 1
  }
}
